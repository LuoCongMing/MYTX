#define UIColorFromRGB(rgbValue)    [UIColor colorWithRed:((float)(((rgbValue) & 0xFF0000) >> 16))/255.0 \
green:((float)(((rgbValue) & 0x00FF00) >> 8))/255.0 \
blue:((float)((rgbValue) & 0x0000FF))/255.0 \
alpha:1.0]

#define  BlueButtonColor  UIColorFromRGB(0x00C7C5)
#define  BlackColor  UIColorFromRGB(0x000000)
#define PingFangSC(a)         [UIFont fontWithName:@"PingFangSC-Regular" size:a]
#define PingFangSCB(a)         [UIFont fontWithName:@"PingFangSC-Semibold" size:a]

#define iPhone_Height       [[UIScreen mainScreen ] bounds ].size.height
#define iPhone_Width        [[UIScreen mainScreen ] bounds ].size.width
#define isIPhoneXAll ([[UIApplication sharedApplication] statusBarFrame].size.height == 44)



